{
  "expo": {
    "scheme": "savy",
    "userInterfaceStyle": "automatic",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "name": "Savy",
    "slug": "savy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/svy.png",
    "splash": {
      "image": "./src/assets/images/sss.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.savy.savy",
      "versionCode": 1
    },
    "experiments": {},
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "13889f80-7a2b-4a6d-91ad-d4b208324348"
      }
    },
    "owner": "savy123",
    "ios": {
      "bundleIdentifier": "com.savy.savy"
    }
  }
}